{
    "name": "backend",
    "version": "1.0.0",
    "main": "dist/server.js",
    "scripts": {
        "dev": "nodemon ./src/server.ts",
        "build": "tsc",
        "test": "jest --coverage",
        "start": "npm run build && ts-node ./dist/server.js",
        "start:local": "cross-env NODE_ENV=dev npm run db:migrate && cross-env NODE_ENV=dev ts-node ./src/server.ts --watch",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --d ./src/database/data-source.ts",
        "typeorm-test": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --d ./src/test-database/testdbconfig.ts",
        "typeorm-without-dataSource": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "create:migration": "npm run typeorm-without-dataSource -- migration:create",
        "db:migrate": "npm run typeorm -- migration:run",
        "db:migrate-test": "npm run typeorm-test -- migration:run",
        "generate:migration": "npm run typeorm -- migration:generate",
        "db:revert": "npm run typeorm -- migration:revert",
        "db:revert:file": "npm run typeorm -- migration:revert -n"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "dependencies": {
        "@apollo/server": "^4.11.3",
        "@types/bcrypt": "^5.0.2",
        "@types/express": "^4.17.21",
        "@types/express-serve-static-core": "^5.0.6",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/node": "^22.13.14",
        "@types/nodemailer": "^6.4.17",
        "apollo-server-express": "^3.13.0",
        "bcrypt": "^5.1.1",
        "body-parser": "^2.2.0",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^16.4.7",
        "express": "^5.1.0",
        "graphql": "^16.10.0",
        "graphql-iso-date": "^3.6.1",
        "graphql-scalars": "^1.24.2",
        "jsonwebtoken": "^9.0.2",
        "nodemailer": "^6.10.0",
        "nodemon": "^3.1.9",
        "pg": "^8.13.3",
        "reflect-metadata": "^0.2.2",
        "tsconfig-paths": "^4.2.0",
        "type-graphql": "^2.0.0-rc.2",
        "typedi": "^0.10.0",
        "typeorm": "^0.3.21",
        "typeorm-typedi-extensions": "^0.4.1",
        "typescript": "^5.8.2",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@types/graphql-iso-date": "^3.4.3",
        "@types/jest": "^29.5.14",
        "@types/supertest": "^6.0.3",
        "cross-env": "^7.0.3",
        "jest": "^29.7.0",
        "supertest": "^7.1.0",
        "ts-jest": "^29.3.1",
        "ts-node": "^10.9.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tring-Santhoshkumar/Asset-Management-Project-Backend-TypeORM.git"
    },
    "bugs": {
        "url": "https://github.com/Tring-Santhoshkumar/Asset-Management-Project-Backend-TypeORM/issues"
    },
    "homepage": "https://github.com/Tring-Santhoshkumar/Asset-Management-Project-Backend-TypeORM#readme",
    "description": ""
}
